# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/public/uploads/*

# Ignore those annoying .DS_Store files on OSX
.DS_Store

# Byebug keeps history
.byebug_history

<fieldset>
  <legend>Resourcing information</legend>
  <%= f.text_field :leader, :label => 'Leader' %>
  <%= f.number_field :adults, :label => 'Adults',          :min => 2, help: 'The minimum number of adults needed to work on this project' %>
  <%= f.number_field :youth,  :label => 'Youth (11-18)',   :min => 0, help: 'The maximum number of youth that could be supervised on this project' %>
  <%= f.number_field :kids,   :label => 'Kids (under 11)', :min => 0, help: 'The maximum number of kids that could be supervised on this project' %>
  <%= f.text_area :materials, :label => 'Materials' %>
</fieldset>
